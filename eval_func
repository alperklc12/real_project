mystr1 = '10 - x = 4'
mystr2 = '1x * 11 = 121'
mystr3 = '1x0 / 3 = 150'
output = '6'

result1 = None
result2 = None
result3 = None

for i in range(10):
    if eval(f'10 - {i}') == eval('4'):
        result1 = i

print(result1)

for j in range(10):
    if eval(f'1{j} * 11') == eval('121'):
        result2 = j

print(result2)

for k in range(1, 10):
    if eval(f'1{k}0 / 3') == eval('50'):
        result3 = k

print(result3)
